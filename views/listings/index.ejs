<% layout("layouts/boilerplate") %>
<%= success %>
<h3>All Listings</h3>

<div class="row row-cols-lg-4 row-cols-md-3 row-cols-sm-1">
  <% for (let listing of allListings) { %>
    <a href="/listings/<%= listing._id %>" class="listing-links">
      <div class="card col">
        <img src="<%= listing.image %>" class="card-img-top" alt="..." style="height: 20rem;">
        <div class="card-img-overlay"></div>
        <div class="card-body">
          <p class="card-text">
            <b><%= listing.title %><br></b>
            &#8377; <%= listing.price %>/night
          </p>
        </div>
      </div>
    </a>
  <% } %>
</div>
